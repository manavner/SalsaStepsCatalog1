{
  "expo": {
    "name": "SalsaStepsCatalog",
    "slug": "salsastepscatalog",
    "version": "4.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "salsastepscatalog",
    "userInterfaceStyle": "light",
    "cli": {
      "appVersionSource": "local"
    },
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "com.avnerman.salsastepscatalog",
      "buildNumber": "4",
      "supportsTablet": true,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "אפליקציה זו עשויה להזדקק לגישה למצלמה לצורך תפקוד מלא של כלל הרכיבים."
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "android": {
      "package": "com.salsastepscatalog.app",
      "versionCode": 4
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "ddefb7da-04db-4d2a-a02d-f8dda9dc9585"
      }
    },
    "owner": "avnerman"
  }
}